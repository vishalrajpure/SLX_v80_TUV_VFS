        <ControlId>btnSave</ControlId>
        <DataBindings />
        <ControlLabelPlacement>none</ControlLabelPlacement>
        <ExtendedProperties />
        <CaptionAlignment>left</CaptionAlignment>
        <StyleScheme />
        <ButtonType>Icon</ButtonType>
        <Image>[Localization!Global_Images:Save_16x16]</Image>
        <OnClientClick />
        <OnClickAction>
          <ResourceKey>btnSave</ResourceKey>
          <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
            <CSharpSnippetActionItem>
              <MethodName />
              <CSharpCodeSnippet>  Sage.Platform.WebPortal.EntityPage page = (Sage.Platform.WebPortal.EntityPage)Page;
    Sage.Entity.Interfaces.IStampTransaction   objInward = this.BindingSource.Current as Sage.Entity.Interfaces.IStampTransaction ;
    //Sage.Entity.Interfaces.IStampMaster objStampMaster = this.BindingSource.Current as Sage.Entity.Interfaces.IStampMaster;
    
    
    //get old Quanity
    Sage.Platform.Data.IDataService service = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Data.IDataService&gt;();
    System.Data.OleDb.OleDbConnection conObj = new System.Data.OleDb.OleDbConnection(service.GetConnectionString());

    string Query = "select isnull(STAMPQUANTITY,0),isnull(AVAILABLEQTY,0)  from stampmaster where STAMPMASTERID='" + Convert.ToString(page.EntityContext.EntityID)   + "'";

    System.Data.OleDb.OleDbDataAdapter dataAdapterObj3 = new System.Data.OleDb.OleDbDataAdapter(Query, conObj);
    System.Data.DataTable dt3 = new System.Data.DataTable();
    dataAdapterObj3.Fill(dt3);

    int oldQuanity;
	int oldstampQty ;
    
    if (dt3.Rows.Count &gt; 0 &amp;&amp; dt3.Rows[0][0].ToString() != "")
    {
        oldstampQty = Convert.ToInt16 (dt3.Rows[0][0].ToString());
        oldQuanity =Convert.ToInt16 (dt3.Rows[0][1].ToString());
    }
    else
    {
        oldQuanity = 0;
		oldstampQty =0;
    }
    
    
    
    //Save

    objInward.StampType ="Inward";
    objInward.StampMasterId =Convert.ToString(page.EntityContext.EntityID);
	objInward.PurchaseFrom=lkpPurchaseFrom.LookupResultValue .ToString ();
	
	  if (!string.IsNullOrEmpty(QFDateTimePicker.DateTimeValue.ToString()))
        {
	
	objInward.PurchaseIssueDate = Convert.ToDateTime(QFDateTimePicker.DateTimeValue );
		}
		
		  if (!string.IsNullOrEmpty(dtExpiryDate.DateTimeValue.ToString()))
        {
    objInward.ExpiryDate = Convert.ToDateTime(dtExpiryDate.DateTimeValue);
		}
	objInward.PurchaseQty =Convert.ToInt16 (PurchaseQty.Text);	 
	objInward.Save() ;
	
    //update master Quantity


    int NewQuantity = oldQuanity + Convert.ToInt16(PurchaseQty.Text);
	 int NewstampQuantity =oldstampQty + Convert.ToInt16(PurchaseQty.Text);
 
    System.Data.OleDb.OleDbConnection conObj1 = new System.Data.OleDb.OleDbConnection(service.GetConnectionString());
    conObj1.Open();
    string query1 = "Update  stampmaster set AVAILABLEQTY =" + NewQuantity + " ,STAMPQUANTITY =  " + NewstampQuantity +" where STAMPMASTERID='" +Convert.ToString (page.EntityContext.EntityID )+ "'";
    System.Data.OleDb.OleDbCommand cmd = new System.Data.OleDb.OleDbCommand(query1, conObj1);
    cmd.ExecuteNonQuery();

    Response.Redirect("StampMaster.aspx");
    
 </CSharpCodeSnippet>
