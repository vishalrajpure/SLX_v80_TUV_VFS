          <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
            <CSharpSnippetActionItem>
              <MethodName />
              <CSharpCodeSnippet>Sage.Entity.Interfaces.IVisitReport VR= this.GetParentEntity() as Sage.Entity.Interfaces.IVisitReport;
Sage.Entity.Interfaces.IVisitReportDocumentReference DR = this.BindingSource.Current as Sage.Entity.Interfaces.IVisitReportDocumentReference;

if (EntityContext.EntityID.ToString().ToUpper().Equals("INSERT") == true)
{
 if (VR.VisitReportDocumentReferences.Count &gt; 0)
 {
foreach (Sage.Entity.Interfaces.IVisitReportDocumentReference obj in VR.VisitReportDocumentReferences)
{
    if (obj.Document == DR.Document)
    {
       throw new Sage.Platform.Application.ValidationException("Document already Present in list"); 
    } 
}}}
DR.VisitReportID=VR.Id.ToString();
DR.Save();
System.Web.HttpContext.Current.Response.Redirect(string.Format("VisitReport.aspx?entityId={1}", "VisitReport", VR.Id.ToString()));    
    </CSharpCodeSnippet>
            </CSharpSnippetActionItem>
