          <Action typeName="Sage.Platform.QuickForms.ActionItems.BusinessRuleActionItem, Sage.Platform.QuickForms">
            <BusinessRuleActionItem>
              <BusinessRule>Delete</BusinessRule>
              <Object>TicketAreaOwner</Object>
              <Parameters />
              <PassResultsToNextAction>False</PassResultsToNextAction>
              <OnComplete typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
                <CSharpSnippetActionItem>
                  <MethodName />
                  <CSharpCodeSnippet>
	//Query the distinct Areas from the AreaCategoryIssue table
	Sage.Platform.RepositoryHelper&lt;Sage.Entity.Interfaces.ITicketAreaOwner&gt; repoHelper = Sage.Platform.EntityFactory.GetRepositoryHelper&lt;Sage.Entity.Interfaces.ITicketAreaOwner&gt;();
    Sage.Platform.Repository.ICriteria criteria = repoHelper.CreateCriteria();
	criteria.AddOrder(repoHelper.EF.Asc("Area"));
	criteria.SetFirstResult(0);
	criteria.SetMaxResults(1);
    criteria.SetProjection(repoHelper.PF.Property("Id"));
    System.Collections.Generic.IList&lt;string&gt; definedAreas = criteria.List&lt;string&gt;();

    if (definedAreas.Count == 0)
    {
        HttpContext.Current.Response.Redirect("~/TicketAreaOwner.aspx?modeId=list", false);
    }
    else
    {
        HttpContext.Current.Response.Redirect(string.Format("~/TicketAreaOwner.aspx?entityId={0}", definedAreas[0].ToString()), false);
    }
</CSharpCodeSnippet>
                </CSharpSnippetActionItem>
              </OnComplete>
            </BusinessRuleActionItem>
          </Action>
